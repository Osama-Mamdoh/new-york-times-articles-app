{
  "name": "new-york-times-articles-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-auth": "cd fake-api-jwt-json-server && npm install && npm run start-auth",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-watch": "ng test --watch --code-coverage",
    "test-coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "lint-ts": "eslint \"src/**/*.ts\" --fix",
    "lint-html": "eslint \"src/**/*.html\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -p tsconfig.doc.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "^13.3.11",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@ngrx/store": "^13.2.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
